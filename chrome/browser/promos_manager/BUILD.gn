source_set("promos_manager") {
  sources = [
    "promos_manager.h",
    "promos_manager.mm",
  ]
  configs += [ "//build/config/compiler:enable_arc" ]
  deps = [
    ":features",
    "//base",
    "//components/prefs",
    "//ios/chrome/browser:pref_names",
  ]
}

source_set("features") {
  sources = [
    "features.h",
    "features.mm",
  ]
  configs += [ "//build/config/compiler:enable_arc" ]
  deps = [ "//base" ]
}
